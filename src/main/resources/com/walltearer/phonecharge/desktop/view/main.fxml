<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.walltearer.phonecharge.desktop.controller.Index">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label text="Battery level:" fx:id="batteryLevelTitle" GridPane.rowIndex="0" GridPane.columnIndex="0" />
      <Label text="... loading" fx:id="batteryLevel" GridPane.rowIndex="0" GridPane.columnIndex="1" />
   </children>
</GridPane>
